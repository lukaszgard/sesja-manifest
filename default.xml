<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="yoctoproject"
        fetch="git://git.yoctoproject.org/"/>

    <remote name="openembedded"
        fetch="git://git.openembedded.org/"/>

    <remote name="github"
        fetch="git://github.com/"/>

    <project name="poky"
        remote="yoctoproject"
        path="yocto_build"
        revision="morty"/>

    <project name="meta-openembedded"
        remote="openembedded"
        path="yocto_build/meta-openembedded"
        revision="morty"/>

    <project name="jumpnow/meta-rpi"
        remote="github"
        path="yocto_build/meta-raspberrypi"
        revision="morty"/>
</manifest>
